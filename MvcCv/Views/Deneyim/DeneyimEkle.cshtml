
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />
<form method="post">
    <label>Başlık</label>
    <input type="text" name="Baslik" class="form-control" />
    <label>Alt Başlık</label>
    <input type="text" name="AltBaslik" class="form-control" />
    <label>Tarih</label>
    <input type="text" name="Tarih" class="form-control" />
    <label>Açıklama</label>
    <textarea name="Aciklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

